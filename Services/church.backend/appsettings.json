  "queries": {
    "access": {
      "employeeLogin": "EXEC stp_access @method='employeeLogin', @email='{0}', @password='{1}'",
      "updatePassword": "EXEC stp_access @method='updatePassword', @userId='{0}', @password='{1}'",
      "recoverPassword": "EXEC stp_access @method='recoverPassword', @email='{0}', @password='{1}'",
      "createUserEmployee": "EXEC stp_access @method='createUserEmployee', @email='{0}', @password='{1}', @catRolesId='{2}', @catStatusId='{3}', @name='{4}', @psurname='{5}', @msurname='{6}', @phone='{7}'"
    },
    "roles": {
      "consultAll": "EXEC stp_roles @method='consultAll'"
    },
    "cathedral": {
      "consultAll": "EXEC stp_cathedral @method = 'consultAll'",
      "create": "EXEC stp_cathedral @method = 'create', @name='{0}', @cityId='{1}', @userId='{2}'"
    },
    "cities": {
      "consultAll": "EXEC stp_cities @method = 'consultAll'",
      "create": "EXEC stp_cities @method = 'create', @city='{0}', @userId='{1}'"
    },
    "civil_status": {
      "consultAll": "EXEC stp_civil_status @method = 'consultAll'",
      "create": "EXEC stp_civil_status @method = 'create', @civilStatus='{0}', @userId='{1}'"
    },
    "days": {
      "consultAll": "EXEC stp_days @method='consultAll'"
    },
    "discounts": {
      "consultAll": "EXEC stp_discounts @method='consultAll'",
      "consultByStatus": "EXEC stp_discounts @method='consultByStatus', @statusId='{0}'",
      "create": "EXEC stp_discounts @method='create', @description='{0}', @percentage='{1}', @startDate='{2}', @endDate='{3}', @userId='{4}'",
      "update": "EXEC stp_discounts @method='update', @description='{0}', @percentage='{1}', @startDate='{2}', @endDate='{3}', @statusId='{4}', @discountId='{5}', @userId='{6}'"
    },
    "federal_tax": {
      "consultAll": "EXEC stp_federal_tax @method='consultAll'",
      "update": "EXEC stp_federal_tax @method='update', @cost='{0}', @userId='{1}'"
    },
    "intents_type": {
      "consultAll": "EXEC stp_intents_type @method='consultAll'",
      "create": "EXEC stp_intents_type @method='create', @intent='{0}', @userId='{1}'"
    },
    "maintenance_fee": {
      "consultAll": "EXEC stp_maintenance_fee @method='consultAll'",
      "update": "EXEC stp_maintenance_fee @method='update', @cost='{0}', @costShared='{1}', @userId='{2}'"
    },
    "misa": {
      "consultAll": "EXEC stp_misa @method='consultAll'",
      "create": "EXEC stp_misa @method='create', @dayId='{0}', @hour='{1}', @userId='{2}'",
      "delete": "EXEC stp_misa @method='delete', @misaId='{0}', @userId='{1}'"
    },
    "state_towns": {
      "consultAll": "EXEC stp_states_towns @method='consultAll'"
    },
    "misa_intents": {
      "consultAll": "EXEC stp_misa_intents @method='consultAll'",
      "consultByStatus": "EXEC stp_misa_intents @method='consultByStatus', @statusId='{0}'",
      "create": "EXEC stp_misa_intents @method='create', @intentId='{0}', @misaId='{1}', @date='{2}',  @mentionPerson='{3}', @applicant='{4}', @phone='{5}', @donation='{6}', @exchangeRate='{7}', @decription='{8}', @userId='{9}'",
      "update": "EXEC stp_misa_intents @method='update', @intentId='{0}', @misaId='{1}', @date='{2}',  @mentionPerson='{3}', @applicant='{4}', @phone='{5}', @donation='{6}', @exchangeRate='{7}', @decription='{8}', @misaIntentId = '{9}', @userId='{10}'"
